<!DOCTYPE html>
<html>
<head>
  <title>Admin</title>
  <link rel="stylesheet" href="/css/style.css">
</head>
<body>
  <h2>Orders</h2>
  <table border="1">
    <tr>
      <th>ID</th><th>Name</th><th>Email</th><th>Service</th><th>Amount</th><th>Date</th>
    </tr>
    <% orders.forEach(o => { %>
      <tr>
        <td><%= o.id %></td>
        <td><%= o.name %></td>
        <td><%= o.email %></td>
        <td><%= o.service %></td>
        <td><%= o.amount %></td>
        <td><%= o.date %></td>
      </tr>
    <% }) %>
  </table>
</body>
</html>
